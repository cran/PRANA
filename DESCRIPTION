Package: PRANA
Type: Package
Title: Pseudo-Value Regression Approach for Network Analysis (PRANA)
Version: 1.0.3
Authors@R: c(
	person("Ahn", "Seungjun", email = "sahn1@ufl.edu",
		 role = c("cre", "aut", "trl"),
